{
    "version": 2,
    "builds": [
        {
            "src" : "socoal_authen/wsgi.py",
            "use" : "@vercel/python",
            "config" : { "maxLambdaSize": "15mb", "runtime": "python3.11.5"}
        },
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static_build",
            "config" : {
                "disDir" : "staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/&1"
        },
        {
            "src": "/(.*)",
            "dest": "socoal_authen/wsgi.py"
        }
    ]
}